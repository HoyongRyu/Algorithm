<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="rhy.sample.user.repository.UserMasterRepository">

	<insert id="insert" parameterType="rhy.sample.user.repository.User">
	    INSERT INTO user (
		    usrId,
		    authFg,
		    crtDt,
		    crtrId,
		    mdfyDt,
		    mdfrId
	    ) 
	    VALUES (
		    #{usrId},
		    #{authFg},
		    #{crtDt},
		    #{crtrId},
		    #{mdfyDt},
		    #{mdfrId}
	    )
	</insert>

</mapper>